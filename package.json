{
  "name": "react-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "algoliasearch": "^4.4.0",
    "axios": "^0.19.2",
    "graphql": "^15.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.11.0",
    "react-instantsearch-dom": "^6.7.0",
    "react-router-dom": "^5.2.0",
    "react-router-tabs": "^1.3.2",
    "react-scripts": "^3.4.3",
    "react-tabs": "^3.1.1",
    "recharts": "^2.1.14"
  },
  "scripts": {
    "start": "npm-run-all -p start:css start:js",
    "build": "npm-run-all build:css build:js",
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "start:css": "postcss src/styles/tailwind.css -o src/main.css -w",
    "build:css": "postcss src/styles/tailwind.css -o src/main.css --env production"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "autoprefixer": "^9.8.6",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.7.3"
  }
}
